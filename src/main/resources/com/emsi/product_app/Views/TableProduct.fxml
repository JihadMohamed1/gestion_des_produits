<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="599.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.emsi.product_app.Views.TableProductController">
    <children>

        <TableView fx:id="tableView" layoutY="34.0" prefHeight="366.0" prefWidth="599.0">
            <columns>
                <TableColumn text="Nom">
                    <cellValueFactory>
                        <PropertyValueFactory property="nom" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn text="Prix">
                    <cellValueFactory>
                        <PropertyValueFactory property="prix" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn text="Description">
                    <cellValueFactory>
                        <PropertyValueFactory property="description" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn text="Quantité">
                    <cellValueFactory>
                        <PropertyValueFactory property="quantite" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn text="Catégorie">
                    <cellValueFactory>
                        <PropertyValueFactory property="categorie" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn text="Supprieme">
                    <cellFactory>
                        <PropertyValueFactory property="Supprieme" />
                    </cellFactory>
                </TableColumn>
                <TableColumn prefWidth="117.0" text="Modifier">
                    <cellFactory>
                        <PropertyValueFactory property="Modifier" />
                    </cellFactory>
                </TableColumn>
            </columns>
        </TableView>
      <Button fx:id="loginButton" layoutX="87.0" layoutY="7.0" mnemonicParsing="false" onAction="#handleAddButton" prefHeight="17.0" prefWidth="100.0" text="Ajoute Product" />
      <Button fx:id="loginButton1" layoutY="7.0" mnemonicParsing="false" onAction="#handllogoutButton" prefHeight="17.0" prefWidth="75.0" text="logout" />
        <Button fx:id="exportButtontxt" layoutX="215.0" layoutY="7.0" mnemonicParsing="false" onAction="#handleExporttxt" prefHeight="17.0" prefWidth="75.0" text="export txt" />
        <Button fx:id="importButtontxt" layoutX="294.0" layoutY="7.0" mnemonicParsing="false" onAction="#handleImporttxt" prefHeight="25.0" prefWidth="75.0" text="import txt" />
        <Button fx:id="exportButtonexl" layoutX="400.0" layoutY="7.0" mnemonicParsing="false" onAction="#handleExportexl" prefHeight="17.0" prefWidth="75.0" text="export exl" />
        <Button fx:id="importButtonexl" layoutX="480.0" layoutY="7.0" mnemonicParsing="false" onAction="#handleImportexl" prefHeight="25.0" prefWidth="75.0" text="import exl" />

    </children>

</AnchorPane>
